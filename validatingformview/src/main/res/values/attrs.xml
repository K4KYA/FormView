<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FormFieldView">
        <attr name="label" format="string" />
        <attr name="placeholder" format="string" />
        <attr name="type" format="enum">
            <enum name="string" value="0" />
            <enum name="number" value="1" />
            <enum name="password" value="2" />
            <enum name="phone" value="3" />
        </attr>
        <attr name="errorMessage" format="string" />
    </declare-styleable>
</resources>